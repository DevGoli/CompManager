<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Office" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="COM_Office"/>
    <singularAlias>Office</singularAlias>
    <pluralAlias>Offices</pluralAlias>
    <iconStyleClass>fa fa-building</iconStyleClass>
    <bizKey expression="Office - {buildingName}"/>
    <attributes>
        <text name="levelUnit">
            <displayName>Level Unit</displayName>
            <length>200</length>
        </text>
        <text required="true" name="buildingName">
            <displayName>Building Name</displayName>
            <length>200</length>
        </text>
        <text name="streetAddress">
            <displayName>Street Address</displayName>
            <length>200</length>
        </text>
        <text required="true" name="suburb">
            <displayName>Suburb</displayName>
            <description><![CDATA[Enter suburb]]></description>
            <length>200</length>
        </text>
        <text required="true" name="postCode">
            <displayName>Post Code</displayName>
            <description><![CDATA[Enter pincode]]></description>
            <length>4</length>
        </text>
        <text required="true" name="phone">
            <displayName>Phone</displayName>
            <length>12</length>
        </text>
    </attributes>
</document>